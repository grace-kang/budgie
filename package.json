{
  "devDependencies": {
    "@prettier/plugin-ruby": "^4.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-ruby": "^0.3.1"
  },
  "scripts": {
    "lint": "bundle exec erb_lint --lint-all --autocorrect",
